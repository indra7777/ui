    <style>
        :root {
            --primary-color: #8e4574;
            --secondary-color: #ff7eff;
            --background-color: #f8f9fa;
            --text-color: #333;
            --card-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);
            --transition: all 0.3s ease;
        }

        body {
            font-family: 'Poppins', sans-serif;
            background: var(--background-color);
            color: var(--text-color);
            line-height: 1.6;
        }

        .header {
            background: #fff;
            box-shadow: 0 2px 15px rgba(0, 0, 0, 0.1);
            padding: 15px 0;
            position: fixed;
            width: 100%;
            top: 0;
            z-index: 100;
        }

        .search-container {
            display: flex;
            gap: 10px;
            align-items: center;
            max-width: 600px;
            margin: 0 auto;
        }

        .search-container input,
        .search-container select {
            padding: 10px;
            border: 1px solid #ddd;
            border-radius: 8px;
            font-size: 14px;
        }

        .search-container button {
            padding: 10px 20px;
            background: var(--primary-color);
            color: white;
            border: none;
            border-radius: 8px;
            cursor: pointer;
            transition: var(--transition);
        }

        .search-container button:hover {
            background: var(--secondary-color);
        }

        .course-item {
            background: #fff;
            border-radius: 15px;
            overflow: hidden;
            box-shadow: var(--card-shadow);
            transition: var(--transition);
            height: 100%;
            display: flex;
            flex-direction: column;
        }

        .course-item:hover {
            transform: translateY(-5px);
        }

        .course-item img {
            width: 100%;
            height: 250px;
            object-fit: cover;
        }

        .course-content {
            padding: 25px;
            flex-grow: 1;
            display: flex;
            flex-direction: column;
        }

        .course-content h3 {
            font-size: 1.4rem;
            margin-bottom: 15px;
        }

        .course-content h3 a {
            color: var(--primary-color);
            text-decoration: none;
            transition: var(--transition);
        }

        .course-content h3 a:hover {
            color: var(--secondary-color);
        }

        .course-content p {
            color: #666;
            margin-bottom: 20px;
            flex-grow: 1;
        }

        .price {
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--primary-color);
        }

        @media (max-width: 768px) {
            .search-container {
                flex-direction: column;
                padding: 0 15px;
            }

            .search-container input,
            .search-container select,
            .search-container button {
                width: 100%;
            }

            .course-item {
                margin-bottom: 20px;
            }
        }

        .vertical-container {
            position: fixed;
            right: -100%;
            top: 0;
            height: 100vh;
            background: white;
            padding: 20px;
            width: 250px;
            box-shadow: -2px 0 10px rgba(0, 0, 0, 0.1);
            transition: var(--transition);
            z-index: 1000;
        }

        .vertical-container.active {
            right: 0;
        }

        .dots-container {
            cursor: pointer;
            position: fixed;
            right: 20px;
            top: 20px;
            z-index: 1001;
        }

        .dot {
            width: 6px;
            height: 6px;
            background: var(--primary-color);
            border-radius: 50%;
            margin: 3px 0;
        }

        #main {
            padding-top: 100px;
        }
    </style>

